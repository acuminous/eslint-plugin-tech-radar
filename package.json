  "description": "An eslint plugin to encourage adherence to an organisational tech radar",
  "main": "index.js",
  "types": "./index.d.ts",  
  "keywords": [
    "eslint",
    "plugin",
    "dependencies",
    "dependency",
    "list",
    "radar",
