    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "~29.3.0",
    "lint-staged": "^15.2.2"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
