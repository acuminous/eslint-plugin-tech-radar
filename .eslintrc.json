{
  "extends": [
    "eslint-config-airbnb-base"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "2022"
  },
  "rules": {
    "arrow-body-style": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": 0,
    "no-console": "error",
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "object-curly-newline": 0,
    "padded-blocks": 0,
    "prefer-destructuring": 0,
    "prefer-regex-literals": 0
  }
}
